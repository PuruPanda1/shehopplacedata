CREATE TABLE `login`.`banner` (`bannerId` INT NOT NULL AUTO_INCREMENT , `bannerImage` VARCHAR(300) NOT NULL , `bannerSubtitle` VARCHAR(100) NOT NULL , `bannerTitle` VARCHAR(100) NOT NULL , `bannerPrice` INT NOT NULL , `bannerUrl` VARCHAR(100) NOT NULL , PRIMARY KEY (`bannerId`)) ENGINE = InnoDB;
CREATE TABLE `login`.`category` (`categoryId` INT NOT NULL AUTO_INCREMENT , `categoryImage` VARCHAR(200) NOT NULL , `categoryTitle` VARCHAR(200) NOT NULL , `categoryNumber` INT NOT NULL , `categoryUrl` VARCHAR(200) NOT NULL , PRIMARY KEY (`categoryId`)) ENGINE = InnoDB;
CREATE TABLE `login`.`fashioncat` (`catId` INT NULL , `catName` VARCHAR(100) NOT NULL ) ENGINE = InnoDB;
CREATE TABLE `login`.`items` (`itemId` INT NOT NULL AUTO_INCREMENT , `itemImage` VARCHAR(100) NOT NULL , `itemTitle` VARCHAR(100) NOT NULL , `itemDesc` VARCHAR(500) NOT NULL , `itemCat` VARCHAR(100) NOT NULL , `salePrice` INT NOT NULL , `usualPrice` INT NOT NULL , PRIMARY KEY (`itemId`)) ENGINE = InnoDB;